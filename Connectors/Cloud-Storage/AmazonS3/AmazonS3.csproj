<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>BeepDataSources.AmazonS3</PackageId>
    <Version>1.0.0</Version>
    <Authors>The-Tech-Idea</Authors>
    <Description>Amazon S3 data source for Beep Data Connectors framework</Description>
    <RepositoryUrl>https://github.com/The-Tech-Idea/BeepDataSources</RepositoryUrl>
    <PackageTags>Beep;DataSource;AmazonS3;AWS;CloudStorage</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Http" Version="8.0.0" />
    <PackageReference Include="Microsoft.Extensions.Http.Polly" Version="8.0.0" />
    <PackageReference Include="System.Text.Json" Version="8.0.0" />
    <PackageReference Include="AWSSDK.S3" Version="3.7.307.27" />
    <PackageReference Include="AWSSDK.Core" Version="3.7.300.27" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\BeepDM\DataManagementModelsStandard\DataManagementModels.csproj" />
    <ProjectReference Include="..\..\BeepDM\DataManagementEngineStandard\DataManagementEngine.csproj" />
  </ItemGroup>

</Project>
