<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>BeepDataSources.OneDrive</PackageId>
    <Version>1.0.0</Version>
    <Authors>The-Tech-Idea</Authors>
    <Description>OneDrive data source for Beep Data Connectors framework</Description>
    <RepositoryUrl>https://github.com/The-Tech-Idea/BeepDataSources</RepositoryUrl>
    <PackageTags>Beep;DataSource;OneDrive;Microsoft;CloudStorage</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Http" Version="10.0.2" />
    <PackageReference Include="Microsoft.Extensions.Http.Polly" Version="10.0.2" />
    <PackageReference Include="System.Text.Json" Version="10.0.2" />
    <PackageReference Include="Microsoft.Graph" Version="5.100.0" />
    <PackageReference Include="Microsoft.Identity.Client" Version="4.81.0" />
    <PackageReference Include="TheTechIdea.Beep.DataManagementEngine" Version="2.0.61" />
    <PackageReference Include="TheTechIdea.Beep.DataManagementModels" Version="2.0.119" />
  </ItemGroup>

</Project>
