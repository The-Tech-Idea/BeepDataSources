<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>BeepDataSources.Box</PackageId>
    <Version>1.0.0</Version>
    <Authors>The-Tech-Idea</Authors>
    <Description>Box data source for Beep Data Connectors framework</Description>
    <RepositoryUrl>https://github.com/The-Tech-Idea/BeepDataSources</RepositoryUrl>
    <PackageTags>Beep;DataSource;Box;CloudStorage</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Box.V2.Core" Version="5.8.0" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="9.0.9" />
    <PackageReference Include="Microsoft.Extensions.Http.Polly" Version="9.0.9" />
    <PackageReference Include="System.Text.Json" Version="9.0.9" />
    <PackageReference Include="TheTechIdea.Beep.DataManagementEngine" Version="2.0.43" />
    <PackageReference Include="TheTechIdea.Beep.DataManagementModels" Version="2.0.88" />
  </ItemGroup>

</Project>
