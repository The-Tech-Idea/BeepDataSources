<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <AssemblyName>TheTechIdea.Beep.Connectors.Loomly</AssemblyName>
    <RootNamespace>TheTechIdea.Beep.Connectors.Loomly</RootNamespace>
    <PackageId>TheTechIdea.Beep.Connectors.Loomly</PackageId>
    <Version>1.0.0</Version>
    <Authors>TheTechIdea</Authors>
    <Company>TheTechIdea</Company>
    <Description>Loomly Social Media Management API Data Source Connector for Beep Framework</Description>
    <RepositoryUrl>https://github.com/The-Tech-Idea/BeepDataSources</RepositoryUrl>
    <PackageTags>beep, datasource, connector, loomly, social-media, social-media-management, api</PackageTags>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Http" Version="9.0.0" />
    <PackageReference Include="System.Text.Json" Version="9.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\BeepDM\DataManagementModelsStandard\DataManagementModels.csproj" />
    <ProjectReference Include="..\..\..\BeepDM\DataManagementEngineStandard\DataManagementEngine.csproj" />
  </ItemGroup>

</Project>