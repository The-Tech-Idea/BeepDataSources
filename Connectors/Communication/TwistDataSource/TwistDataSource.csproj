<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>Beep.Connectors.Communication.Twist</PackageId>
    <Version>1.0.0</Version>
    <Authors>The-Tech-Idea</Authors>
    <Company>The-Tech-Idea</Company>
    <Description>Twist communication platform data source for Beep Data Connectors</Description>
    <RepositoryUrl>https://github.com/The-Tech-Idea/BeepDataSources</RepositoryUrl>
