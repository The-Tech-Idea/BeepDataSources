<Project Sdk="Microsoft.NET.Sdk">
<PropertyGroup>
    <OutputType>Library</OutputType>
 <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>TheTechIdea.Beep.Database</RootNamespace>
  </PropertyGroup>
<ItemGroup>
		<Compile Include="..\..\DataSourcesPlugins\SQLServerDataSource\*.cs" />
		<EmbeddedResource Include="..\..\DataSourcesPlugins\SQLServerDataSource\**\*.resx" />
</ItemGroup>
<ItemGroup>
  <PackageReference Include="System.Data.SqlClient" Version="4.8.5" />
  <PackageReference Include="TheTechIdea.Beep.DataManagementEngine" Version="1.0.1" />
  <PackageReference Include="TheTechIdea.Beep.DataManagementModels" Version="1.0.1" />
</ItemGroup>
<Target Name="PostBuild" AfterTargets="PostBuildEvent">
	<copy SourceFiles="$(ProjectDir)$(OutputPath)$(TargetName).dll" DestinationFolder="$(ProjectDir)..\OutputDLL\$(TargetFramework)\" />
</Target>

</Project>
