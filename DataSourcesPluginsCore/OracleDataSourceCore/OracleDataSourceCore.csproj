<Project Sdk="Microsoft.NET.Sdk">
<PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>TheTechIdea.Beep.Database</RootNamespace>
  <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
 </PropertyGroup>
<ItemGroup>
		<Compile Include="..\..\DataSourcesPlugins\OracleDataSource\*.cs" />
		<EmbeddedResource Include="..\..\DataSourcesPlugins\OracleDataSource\**\*.resx" />
</ItemGroup>
<ItemGroup>
  <PackageReference Include="Oracle.ManagedDataAccess.Core" Version="3.21.50" />
  <PackageReference Include="TheTechIdea.Beep.DataManagementEngineNet6" Version="1.0.0.3" />
  <PackageReference Include="TheTechIdea.Beep.DataManagementModelsNet6" Version="1.0.0.3" />
</ItemGroup>

<Target Name="PostBuild" AfterTargets="PostBuildEvent">
  <Exec Command="copy $(ProjectDir)\bin\Debug\net6.0\$(TargetName).dll  $(ProjectDir)\..\OutputDLL\$(TargetName).dll" />
</Target>
</Project>
