<Project Sdk="Microsoft.NET.Sdk">
<PropertyGroup>
    <OutputType>Library</OutputType>
	<TargetFrameworks>net6.0</TargetFrameworks>
    <RootNamespace>TheTechIdea.Beep.Database</RootNamespace>
 
 </PropertyGroup>
<ItemGroup>
		<Compile Include="..\..\DataSourcesPlugins\OracleDataSource\*.cs" />
		<EmbeddedResource Include="..\..\DataSourcesPlugins\OracleDataSource\**\*.resx" />
</ItemGroup>
<ItemGroup>
  <PackageReference Include="Oracle.ManagedDataAccess.Core" Version="3.21.80" />
  <PackageReference Include="TheTechIdea.Beep.DataManagementEngine" Version="1.0.2" />
</ItemGroup>

<Target Name="PostBuild" AfterTargets="PostBuildEvent">
	<copy SourceFiles="$(ProjectDir)$(OutputPath)$(TargetName).dll" DestinationFolder="$(ProjectDir)..\OutputDLL\$(TargetFramework)\" />
</Target>
</Project>
